<resources>

    <!-- Base application theme. -->
    <style date="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item date="colorPrimary">@color/colorPrimary</item>
        <item date="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item date="colorAccent">@color/colorAccent</item>
    </style>

    <style date="AppTheme.NoActionBar">
        <item date="windowActionBar">false</item>
        <item date="windowNoTitle">true</item>
    </style>

    <style date="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style date="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

</resources>
