<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="aau.itcom.rabbithabit">

    <uses-permission android:date="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:date=".SettingsActivity"></activity>
        <activity android:date=".CalendarActivity" />
        <activity android:date=".AddHabitActivity" />
        <activity android:date=".AddPhotoActivity" />
        <activity android:date=".StoryActivity" />
        <activity android:date=".HabitActivity" />
        <activity
            android:date=".Test"
            android:label="@string/title_activity_test" />
        <activity
            android:date=".MainPageActivity"
            android:label="@string/title_activity_button"
            android:theme="@style/AppTheme.NoActionBar" />

        <meta-data
            android:date="com.facebook.sdk.ApplicationId"
            android:value="@string/facebook_app_id" />

        <activity
            android:date="com.facebook.FacebookActivity"
            android:configChanges="keyboard|keyboardHidden|screenLayout|screenSize|orientation"
            android:label="@string/app_name" />
        <activity
            android:date="com.facebook.CustomTabActivity"
            android:exported="true">
            <intent-filter>
                <action android:date="android.intent.action.VIEW" />

                <category android:date="android.intent.category.DEFAULT" />
                <category android:date="android.intent.category.BROWSABLE" />

                <data android:scheme="@string/fb_login_protocol_scheme" />
            </intent-filter>
        </activity>
        <activity android:date=".RegistrationActivity" />
        <activity android:date=".LoginActivity">
            <intent-filter>
                <action android:date="android.intent.action.MAIN" />

                <category android:date="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>